# add a access code to lock your lobe-chat application, you can set a long password to avoid leaking. If this value contains a comma, it is a password array.
# ACCESS_CODE=lobe66

# Specify your API Key selection method, currently supporting `random` and `turn`.
# API_KEY_SELECT_MODE=random


########################################
########## AI Provider Service #########
########################################

### OpenAI ###

# you openai api key
OPENAI_API_KEY=sk-xxxxxxxxx

# use a proxy to connect to the OpenAI API
# OPENAI_PROXY_URL=https://api.openai.com/v1

# add your custom model name, multi model separate by comma. for example gpt-3.5-1106,gpt-4-1106
# OPENAI_CUSTOM_MODELS=gpt-3.5-turbo-1106,gpt-4-1106-preview

### Azure OpenAI ###

# Enable Azure OpenAI Service
# ENABLED_AZURE_OPENAI=1

# Azure OpenAI API Key
# AZURE_API_KEY=xxxxxxxxx

# Azure OpenAI API Version
# AZURE_API_VERSION=2023-08-01-preview

# Azure OpenAI Endpoint
# AZURE_ENDPOINT=https://xxxxxxxxx.openai.azure.com

### Anthropic ###

# Enable Anthropic Claude
# ENABLED_ANTHROPIC=1

# Anthropic API Key
# ANTHROPIC_API_KEY=sk-ant-xxxxxxxxx

### Google ###

# Enable Google Gemini
# ENABLED_GOOGLE=1

# Google API Key
# GOOGLE_API_KEY=xxxxxxxxx

### Ollama ###

# Enable Ollama
# ENABLED_OLLAMA=1

# Ollama API URL
# OLLAMA_PROXY_URL=http://localhost:11434/v1

########################################
########## Database Configuration ######
########################################

# Database URL for PostgreSQL
# DATABASE_URL=postgresql://username:password@localhost:5432/lobechat

# Enable database driver
# DATABASE_DRIVER=node

########################################
########## Authentication ##############
########################################

# NextAuth Secret
# NEXTAUTH_SECRET=your-secret-key

# NextAuth URL
# NEXTAUTH_URL=http://localhost:3210

########################################
########## File Upload #################
########################################

# S3 Configuration for file uploads
# S3_ACCESS_KEY_ID=your-access-key
# S3_SECRET_ACCESS_KEY=your-secret-key
# S3_ENDPOINT=your-s3-endpoint
# S3_BUCKET=your-bucket-name
# S3_PUBLIC_DOMAIN=your-public-domain

########################################
########## Analytics ####################
########################################

# Enable analytics
# NEXT_PUBLIC_ANALYTICS_VERCEL=1
# NEXT_PUBLIC_ANALYTICS_POSTHOG=1

########################################
########## Other Configuration #########
########################################

# App URL
# APP_URL=http://localhost:3210

# Enable telemetry
# TELEMETRY=1